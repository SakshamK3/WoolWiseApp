<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/clients"
    android:textAlignment="center"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/supply_text4"
        android:layout_width="124dp"
        android:layout_height="38dp"
        android:layout_marginTop="400dp"
        android:text="My Options :"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/line_11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.024"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/username_text"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/supply_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Revenue"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/notificationbox2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.873"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.945" />

    <TextView
        android:id="@+id/supply_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="48dp"
        android:text=" Bill"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/notificationbox2"
        app:layout_constraintEnd_toStartOf="@+id/supply_text3"
        app:layout_constraintHorizontal_bias="0.983"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bills"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/pending_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pending"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/notificationbox2"
        app:layout_constraintEnd_toStartOf="@+id/supply_text2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/supply"
        app:layout_constraintTop_toBottomOf="@+id/supply_text4"
        app:layout_constraintVertical_bias="0.69" />

    <View
        android:id="@+id/rectangle"
        android:layout_width="423dp"
        android:layout_height="16dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#D9D9D9"
        app:layout_constraintBottom_toTopOf="@+id/line_11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.001" />

    <View
        android:id="@+id/notificationbox2"
        android:layout_width="466dp"
        android:layout_height="67dp"
        android:background="@drawable/rectangle_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/line_1"
        android:layout_width="425dp"
        android:layout_height="2dp"
        android:background="@drawable/line"
        android:elevation="4dp"
        app:layout_constraintBottom_toTopOf="@+id/bills"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/username_text"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/line_11"
        android:layout_width="421dp"
        android:layout_height="2dp"
        android:background="@drawable/line"
        android:elevation="4dp"
        app:layout_constraintBottom_toTopOf="@+id/notificationbox2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/marketplace"
        android:layout_width="75dp"
        android:layout_height="54dp"
        android:src="@drawable/market_place"
        app:layout_constraintBottom_toBottomOf="@+id/notificationbox2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line_11"
        app:layout_constraintVertical_bias="0.75" />

    <ImageView
        android:id="@+id/clients_contact"
        android:layout_width="67dp"
        android:layout_height="51dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/clients_contact"
        app:layout_constraintBottom_toBottomOf="@+id/notificationbox2"
        app:layout_constraintEnd_toStartOf="@+id/history"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/marketplace"
        app:layout_constraintTop_toTopOf="@+id/line_11"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/qr"
        android:layout_width="73dp"
        android:layout_height="61dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/qr_scan"
        app:layout_constraintBottom_toBottomOf="@+id/notificationbox2"
        app:layout_constraintEnd_toStartOf="@+id/notification"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toEndOf="@+id/clients_contact"
        app:layout_constraintTop_toTopOf="@+id/line_11"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/notification"
        android:layout_width="57dp"
        android:layout_height="58dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/notification_unread"
        app:layout_constraintBottom_toBottomOf="@+id/notificationbox2"
        app:layout_constraintEnd_toEndOf="@+id/history"
        app:layout_constraintHorizontal_bias="0.594"
        app:layout_constraintStart_toEndOf="@+id/clients_contact"
        app:layout_constraintTop_toTopOf="@+id/line_11"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/history"
        android:layout_width="59dp"
        android:layout_height="52dp"
        android:src="@drawable/history"
        app:layout_constraintBottom_toBottomOf="@+id/notificationbox2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toEndOf="@+id/marketplace"
        app:layout_constraintTop_toTopOf="@+id/line_11"
        app:layout_constraintVertical_bias="0.64" />

    <TextView
        android:id="@+id/username_text"
        android:layout_width="270dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/menu_icon"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:padding="8dp"
        android:src="@drawable/ic_menu"
        app:layout_constraintBottom_toTopOf="@+id/line_11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.956"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle"
        app:layout_constraintVertical_bias="0.04" />

    <ImageView
        android:id="@+id/bills"
        android:layout_width="85dp"
        android:layout_height="64dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/pending_bill"
        app:layout_constraintBottom_toTopOf="@+id/pending_text2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/supply_text4"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/supply"
        android:layout_width="107dp"
        android:layout_height="65dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/pending_supply"
        app:layout_constraintBottom_toTopOf="@+id/pending_text"
        app:layout_constraintEnd_toStartOf="@+id/bills"
        app:layout_constraintHorizontal_bias="0.358"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/supply_text4"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/revenue"
        android:layout_width="85dp"
        android:layout_height="64dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="460dp"
        android:src="@drawable/revenue"
        app:layout_constraintBottom_toTopOf="@+id/supply_text3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.817"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_icon"
        app:layout_constraintVertical_bias="0.666" />

    <ImageView
        android:id="@+id/pic1"
        android:layout_width="400dp"
        android:layout_height="163dp"
        android:src="@drawable/homepage_pic1"
        app:layout_constraintBottom_toTopOf="@+id/pic2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line_1" />

    <ImageView
        android:id="@+id/pic2"
        android:layout_width="416dp"
        android:layout_height="166dp"
        android:src="@drawable/homepage_pic2"
        app:layout_constraintBottom_toTopOf="@+id/bills"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line_1"
        app:layout_constraintVertical_bias="0.706" />

    <TextView
        android:id="@+id/supply_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Supply"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/clients_contact"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.221"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pic2"
        app:layout_constraintVertical_bias="0.742" />

    <TextView
        android:id="@+id/pending_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:text="Pending"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/marketplace"
        app:layout_constraintEnd_toStartOf="@+id/supply_text"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/supply_text4"
        app:layout_constraintVertical_bias="0.962" />

</androidx.constraintlayout.widget.ConstraintLayout>
